<h2 class="page-header">Edit Book</h2>
<form action="http://localhost:3000/books/<%= book.id %>?_method=PUT" method="POST">
  <%- include('_form_fields') %>
  <div class="form-row form-row-end btn-row">
    <a class="btn btn-danger" href="http://localhost:3000/books">Cancel</a>
    <button class="btn btn-primary" type="submit">Update</button>
  </div>
</form>